<?xml version="1.0"?>
<project default="sysGenerator" basedir=".">
	<property name="generated.source.dir" value="${basedir}" />
	<target name="sysGenerator" description="mybatis-generator">
		<taskdef name="sysGenerator" classname="org.mybatis.generator.ant.GeneratorAntTask" classpath="../src/main/java" />
		<sysGenerator overwrite="true" configfile="sysGenerator.xml" verbose="false">
			<propertyset>
				<propertyref name="generated.source.dir" />
			</propertyset>
		</sysGenerator>
	</target>
</project>